#include <iostream>
using namespace std;

void processArr(int a[][2]) {
 for (int i = 0; i < 2; i++) {
  for (int j = 0; j < 2; j++) {
    cout << a[i][j] << " ";
  }
  cout << endl;
  }
}

int main() {
 int arr[2][2];
 arr[0][0] = 0;
 arr[0][1] = 1;
 arr[1][0] = 2;
 arr[1][1] = 3;
 processArr(arr);

}
